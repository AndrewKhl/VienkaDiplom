<Window x:Class="Diplom.SetSynhronizationStation"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Diplom"
        mc:Ignorable="d"
        Icon="pack://application:,,,/Resources/icon.png"
        WindowStartupLocation="CenterScreen"
        Title="МД1-1Р::Синхронизация по:" Height="120" Width="320">

    <StackPanel>
        <TextBlock Text="Синхронизация по:" HorizontalAlignment="Left" Margin="20 5 0 5"/>
        <ComboBox Name="selectType" FontSize="12" Width="270" Height="22">
            <ComboBoxItem>Внутренняя</ComboBoxItem>
            <ComboBoxItem>РК</ComboBoxItem>
        </ComboBox>

        <Button Click="SetSynhroniztionStation"  Height="20" Width="80" HorizontalAlignment="Right" Margin="20 10">Применить</Button>
    </StackPanel>
</Window>
